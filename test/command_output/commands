#Format:
#<filename>: <corresponding commandline invocation>
qstat_Q_f: qstat -Q -f
qstat_B_f: qstat -B -f
qstat: qstat
pbsnodes_a: pbsnodes -a
wsrf_query: wsrf-query -a -s https://osg-gw-5.t2.ucsd.edu:9443/wsrf/services/ManagedJobFactoryService --key '{http://www.globus.org/namespaces/2004/10/gram/job}ResourceID' Fork "//*[local-name()='version']"
condor_status_xml: condor_status -xml
condor_status_xml: condor_status -xml -constraint 'TRUE'
condor_status_submitter_xml: condor_status -submitter -xml
condor_version: condor_version
condor_group: condor_config_val GROUP_NAMES
GROUP_PRIO_FACTOR_group_cdf: condor_config_val GROUP_PRIO_FACTOR_group_cdf
GROUP_PRIO_FACTOR_group_cms: condor_config_val GROUP_PRIO_FACTOR_group_cms
GROUP_PRIO_FACTOR_group_cmsprod: condor_config_val GROUP_PRIO_FACTOR_group_cmsprod
GROUP_PRIO_FACTOR_group_lcgadmin: condor_config_val GROUP_PRIO_FACTOR_group_lcgadmin
GROUP_PRIO_FACTOR_group_ligo: condor_config_val GROUP_PRIO_FACTOR_group_ligo
GROUP_QUOTA_group_cdf: condor_config_val GROUP_QUOTA_group_cdf
GROUP_QUOTA_group_cms: condor_config_val GROUP_QUOTA_group_cms
GROUP_QUOTA_group_cmsprod: condor_config_val GROUP_QUOTA_group_cmsprod
GROUP_QUOTA_group_lcgadmin: condor_config_val GROUP_QUOTA_group_lcgadmin
GROUP_QUOTA_group_ligo: condor_config_val GROUP_QUOTA_group_ligo
COLLECTOR_HOST: condor_config_val COLLECTOR_HOST
bjobs_u_all_r: bjobs -u all -r
bhosts: bhosts
bqueues_l: bqueues -l
lsid: lsid
bmgroup_r: bmgroup -r
bugroup_r: bugroup -r
srm_ping: srm-ping srm://osgserv04.slac.stanford.edu:8443/srm/v2/server -proxyfile /tmp/http_proxy
grid_proxy_info: grid-proxy-info -f /tmp/http_proxy
grid_proxy_init: grid-proxy-init -valid 00:05 -cert /etc/grid-security/http/httpcert.pem -key /etc/grid-security/http/httpkey.pem -out /tmp/http_proxy
qstat_help: qstat -help
qstat_xml: qstat -xml
qstat_f_xml: qstat -f -xml
qconf_sq_all: qconf -sq all.q
qconf_sq_chandra: qconf -sq chandra
qconf_sq_geo: qconf -sq geo
qconf_sq_lsdyna: qconf -sq lsdyna
qconf_sq_myrinet: qconf -sq myrinet
qconf_sq_pf24h: qconf -sq pf24h
qconf_sq_schorr_a: qconf -sq schorr_a
qconf_sql: qconf -sql
